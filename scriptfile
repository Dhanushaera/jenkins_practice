pipeline {
  agent any
  parameters {
      string(name: 'Dhanush', description: 'Please tell me your name?')
 
    }
  stages {
    stage('version') {
      steps {
        sh 'python3 --version'
      }
    }
    stage('hello') {
      steps {
        sh 'python3 hello.py arg1 arg2 arg3 '
      }
    }
    stage('Printing Parameters') {
      steps {
          echo "python commandline.py"
      
     }
   }
  }
}
